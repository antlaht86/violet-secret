{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "deploy": "npm run build && npm run start",
    "dev": "concurrently \"npm run dev:css\" \"node -r dotenv/config node_modules/.bin/remix dev\" ",
    "postinstall": "remix setup node",
    "start": "node -r dotenv/config node_modules/.bin/remix-serve api/_build",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "push": "git add . && git commit -m '.' && git push origin master"
  },
  "dependencies": {
    "@remix-run/react": "^2.14.0",
    "@remix-run/serve": "^2.14.0",
    "@remix-run/vercel": "^1.19.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^14.3.2",
    "lucide-react": "^0.460.0",
    "mongodb": "^6.10.0",
    "postcss": "^8.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "react-use-clipboard": "^1.0.7",
    "remix": "^2.14.0",
    "tiny-invariant": "^1.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@remix-run/v1-route-convention": "^0.1.4",
    "@types/crypto-js": "^4.1.0",
    "@types/memory-cache": "^0.2.2",
    "@types/react": "^18.3.12",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^17.0.9",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.0.0",
    "tailwindcss": "^3.0.17",
    "typescript": "^5.1.0"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
